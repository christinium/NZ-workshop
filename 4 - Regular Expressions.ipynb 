        "colab": {}
      },
      "cell_type": "code",
      "source": [
        "#Printing out lines with RUL\n",
        "\n",
        "regular_expression_query = r'RLL.*'\n",
        "  \n",
        "for i in range(len(resp_reports)):\n",
        "  hit = re.search(regular_expression_query,resp_reports[\"TEXT\"][i]) \n",
        "  if hit:\n",
        "    print(hit.group())\n",
        "  else:\n",
        "    print('No hit for the regular expression')\n"
